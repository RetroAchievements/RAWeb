{
    "private": true,
    "name": "raweb",
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "vite",
        "build": "vite build && vite build --ssr",
        "tsc": "tsc --noEmit --skipLibCheck",
        "lint": "eslint public/js resources/js postcss.config.js tailwind.config.js vite.config.ts --cache",
        "lint:fix": "eslint public/js resources/js postcss.config.js tailwind.config.js vite.config.ts --fix",
        "test": "TZ=UTC vitest run --coverage --coverage.reporters=text --reporter=dot",
        "test:coverage": "TZ=UTC vitest --ui --coverage --reporter=dot",
        "test:run": "TZ=UTC vitest --run",
        "test:watch": "TZ=UTC vitest",
        "verify": "npm run lint:fix && npm run tsc && npm run test",
        "analyze": "cross-env ANALYZE=true pnpm prod",
        "crowdin:download": "tsx resources/js/tools/crowdin-download.ts",
        "crowdin:upload": "tsx resources/js/tools/crowdin-upload.ts"
    },
