        "eslint": "^8.4.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.20.2",
        "laravel-echo": "^1.4.0",
        "laravel-vite-plugin": "^0.7.0",
        "lodash": "^4.17.19",
